<div class="navbar-custom">
    <ul class="list-unstyled topbar-right-menu float-right mb-0">
        {% if admin_notification_enable() %}
            {% include "@UmbrellaAdmin/Notification/widget.html.twig" %}
        {% endif %}
        {% include "@UmbrellaAdmin/Include/userinfo.html.twig" %}
    </ul>

    <button class="button-menu-mobile open-left disable-btn">
        <i class="uil-list-ul"></i>
    </button>

    <div class="app-breadcrumb">
        {% if breadcrumb is not defined %}
            {% set breadcrumb = menu_get_breadcrumb(sidebar_alias | default('admin_sidebar')) %}
        {% endif %}
        {{ breadcrumb_render(breadcrumb) }}
    </div>

</div>