{% extends "@UmbrellaAdmin/Layout/layout.html.twig" %}

{% block content %}
    <form class="form-label-right"
          enctype="multipart/form-data"
          method="POST"
          data-mount="Form">

        <div class="d-flex mb-3">
            <span class="ml-auto"></span>
            <button type="submit" class="btn btn-primary">
                <i class="fa fa-check"></i> {{ 'action.save' | trans }}
            </button>
        </div>

        <div class="card">
            <div class="card-body">
                {{ form_errors(form) }}
                {{ form_rest(form) }}
            </div>
        </div>
    </form>
{% endblock %}
